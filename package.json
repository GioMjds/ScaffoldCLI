{
  "name": "giomjds-template-cli",
  "version": "1.2.7",
  "description": "CLI to scaffold projects from GioMjds project templates per stack",
  "main": "dist/src/index.js",
  "bin": {
    "scaffold": "dist/src/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "nodemon --exec ts-node src/index.ts",
    "prepublishOnly": "npm run build",
    "scaffold": "npx giomjds-template-cli"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "cli-spinner": "^0.2.10",
    "commander": "^14.0.0",
    "figlet": "^1.9.1",
    "gradient-string": "^3.0.0",
    "inquirer": "^12.9.4"
  },
  "devDependencies": {
    "@types/cli-spinner": "^0.2.3",
    "@types/figlet": "^1.7.0",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
